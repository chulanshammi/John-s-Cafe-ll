<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.design.vidController">
    <children>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="422.0" prefWidth="598.0">
            <children>
                <Text fill="#942f2f" layoutX="134.0" layoutY="39.0" strokeType="OUTSIDE" strokeWidth="0.0" text="VIEWING THE ITEMS TABLE">
                    <font>
                        <Font name="Berlin Sans FB" size="28.0" />
                    </font>
                </Text>
                <Separator layoutY="370.0" prefHeight="0.0" prefWidth="598.0" />
            <Button fx:id="vidBack" layoutX="543.0" layoutY="383.0" mnemonicParsing="false" onMouseClicked="#backClick" text="Back" />
            <TableView fx:id="vidTable" layoutX="4.0" layoutY="53.0" prefHeight="313.0" prefWidth="590.0">
               <columns>
                  <TableColumn fx:id="column1" prefWidth="49.0" text="Item ID" />
                  <TableColumn fx:id="column2" prefWidth="87.0" text="Name" />
                  <TableColumn fx:id="column3" prefWidth="72.0" text="Brand" />
                  <TableColumn fx:id="column4" prefWidth="89.0" text="Price(Rs.)" />
                  <TableColumn fx:id="column5" prefWidth="60.0" text="Quantity" />
                  <TableColumn fx:id="column6" prefWidth="71.0" text="Total Price" />
                  <TableColumn fx:id="column7" prefWidth="65.0" text="Category" />
                  <TableColumn fx:id="column8" prefWidth="96.0" text="Purchased Date" />
               </columns>
            </TableView>
            </children>
        </AnchorPane>
    </children>
</VBox>
