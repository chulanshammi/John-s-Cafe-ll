<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.design.vrlController">
    <children>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="422.0" prefWidth="598.0">
            <children>
                <Text fill="#942f2f" layoutX="101.0" layoutY="39.0" strokeType="OUTSIDE" strokeWidth="0.0" text="RANDOMLY SELECTED DEALERS">
                    <font>
                        <Font name="Berlin Sans FB" size="28.0" />
                    </font>
                </Text>
                <Separator layoutY="370.0" prefHeight="0.0" prefWidth="598.0" />
                <Button layoutX="536.0" layoutY="383.0" mnemonicParsing="false" onMouseClicked="#vrlSelectClick" text="Select" />
            <Button fx:id="vrlBack" layoutX="482.0" layoutY="383.0" mnemonicParsing="false" onMouseClicked="#backClick" text="Back" />
            <TableView fx:id="vrlTable" layoutX="84.0" layoutY="45.0" prefHeight="250.0" prefWidth="431.0">
               <columns>
                  <TableColumn fx:id="column1" prefWidth="133.0" text="Name" />
                  <TableColumn fx:id="column2" prefWidth="143.0" text="Telephone" />
                  <TableColumn fx:id="column3" prefWidth="154.0" text="Location" />
               </columns>
            </TableView>
            <Text fx:id="vrlText" layoutX="14.0" layoutY="400.0" strokeType="OUTSIDE" strokeWidth="0.0" />
            <TextField fx:id="dealerName" layoutX="249.0" layoutY="316.0" prefHeight="25.0" prefWidth="266.0" />
            <Text layoutX="165.0" layoutY="333.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Dealer Name :" />
            </children>
        </AnchorPane>
    </children>
</VBox>
